{{#if this.keycloakSession.ready}}
  {{#if this.keycloakSession.authenticated}}
    <a href="#" {{on "click" (fn this.logout)}}>
      <i class="fa fa-sign-out"></i> Sign out
    </a>
  {{else}}
    <a href="#" {{on "click" (fn this.login)}}>
      <i class="fa fa-sign-in"></i> Sign in
    </a>
  {{/if}}
{{else}}
  No session
{{/if}}
